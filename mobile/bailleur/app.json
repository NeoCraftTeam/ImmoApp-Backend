{
  "expo": {
    "name": "KeyHome Owner",
    "slug": "keyhome-owner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSAllowsArbitraryLoadsInWebContent": true,
          "NSExceptionDomains": {
            "keyhomeback.neocraft.dev": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionRequiresForwardSecrecy": true
            },
            "keyhome.test": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionRequiresForwardSecrecy": false
            }
          }
        },
        "NSCameraUsageDescription": "Cette application utilise la caméra pour ajouter des photos aux annonces.",
        "NSPhotoLibraryUsageDescription": "Cette application accède à vos photos pour les ajouter aux annonces.",
        "NSLocationWhenInUseUsageDescription": "Cette application utilise votre position pour localiser les biens immobiliers."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "cm.neocraft.keyhome.bailleur",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "aa2a861b-6ae4-481a-bc53-0fdf73421cb4"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-web-browser"
    ]
  }
}
